{
  "name": "@jsonresume/theme-papirus",
  "version": "1.0.1",
  "description": "Papirus theme for JSON Resume",
  "author": "Konstantinos Alexiou",
  "repository": {
    "type": "git",
    "url": "https://github.com/konalexiou/jsonresume-theme-papirus"
  },
