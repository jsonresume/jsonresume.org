{
  "name": "jsonresume-theme-data-driven",
  "version": "0.1.0",
  "description": "Data-driven analytical theme with sky blue accent - built with @resume/core",
  "type": "module",
  "main": "./src/index.jsx",
  "exports": {
    ".": {
      "import": "./src/index.jsx",
      "default": "./src/index.jsx"
    },
    "./dist": {
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "vite build",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "jsonresume",
    "theme",
    "resume",
    "cv",
    "data-driven",
    "analytical",
    "metrics",
    "compact"
  ],
  "dependencies": {
    "@jsonresume/core": "workspace:*",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.4",
    "vite": "^5.4.21",
    "vitest": "^1.6.0"
  },
  "peerDependencies": {
    "styled-components": "^6"
  }
}
