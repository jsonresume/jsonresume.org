{
  "name": "jsonresume-theme-sidebar",
  "version": "0.1.0",
  "description": "Two-column sidebar resume theme with dark sidebar and light main content - ATS-friendly",
  "type": "module",
  "main": "./src/index.jsx",
  "exports": {
    ".": {
      "import": "./src/index.jsx",
      "default": "./src/index.jsx"
    },
    "./dist": {
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "build": "vite build"
  },
  "keywords": [
    "jsonresume",
    "theme",
    "resume",
    "cv",
    "sidebar",
    "two-column",
    "professional",
    "ats-friendly"
  ],
  "dependencies": {
    "@jsonresume/core": "workspace:*",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "vitest": "^1.6.0",
    "@vitejs/plugin-react": "^4.3.4",
    "vite": "^5.4.21"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  }
}
