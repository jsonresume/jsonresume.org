{
  "name": "jsonresume-theme-writers-portfolio",
  "version": "0.1.0",
  "description": "Literary and elegant resume theme for content writers, editors, and communications professionals. Features transitional serif typography, generous spacing, and prose-like readability.",
  "type": "module",
  "main": "./src/index.jsx",
  "exports": {
    ".": {
      "import": "./src/index.jsx",
      "default": "./src/index.jsx"
    },
    "./dist": {
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "build": "vite build"
  },
  "keywords": [
    "jsonresume",
    "theme",
    "resume",
    "cv",
    "writer",
    "portfolio",
    "literary",
    "elegant",
    "serif",
    "editorial",
    "content",
    "communications",
    "ats-friendly"
  ],
  "dependencies": {
    "@jsonresume/core": "workspace:*",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "vitest": "^1.6.0",
    "@vitejs/plugin-react": "^4.3.4",
    "vite": "^5.4.21"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  }
}
